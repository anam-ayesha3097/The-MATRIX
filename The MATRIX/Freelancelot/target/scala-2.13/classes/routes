# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# An example controller showing a sample home page
GET     /freelancelot               controllers.HomeController.freelancelot(request: Request, searchTerm:String ?="",sessionID:String ?="")
GET    /myName                      controllers.HomeController.sayHello(name:String)
GET    /ws                          controllers.HomeController.ws(searchTerm:String)
GET    /IndvWordStats/:searchTerm   controllers.HomeController.projectWordStats(searchTerm: String)
GET    /wordstats                   controllers.HomeController.wordStats(searchTerm: String ?="")
GET    /skills                              controllers.HomeController.skills(s: String)
GET    /ownerInformation/:ownerId           controllers.HomeController.ownerInformation(ownerId:String)
# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
GET     /assets/*file               controllers.Assets.at(path = "/public", file)
